{
  "name": "xpack",
  "version": "0.1.1",
  "description": "The xPack tools collection",
  "main": "./lib/module.js",
  "directories": {
    "bin": "./bin",
    "lib": "./lib",
    "test": "./test"
  },
  "bin": {},
  "scripts": {
    "prepublishOnly": "standard && npm run test-tap -s",
    "test": "standard && npm run test-tap -s",
    "test-coverage": "tap --coverage --reporter=classic --timeout 600 --no-color \"test/tap/*.js\"",
    "test-tap": "tap --reporter=classic --timeout 300 --no-color \"test/tap/*.js\"",
    "tap": "tap --reporter=spec --timeout 300 --no-color",
    "fix": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpack/xpack-js.git"
  },
  "keywords": [
    "xpack",
    "tools"
  ],
  "author": {
    "name": "Liviu Ionescu",
    "email": "ilg@livius.net",
    "url": "http://liviusdotnet.wordpress.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xpack/xpack-js/issues"
  },
  "homepage": "https://github.com/xpack/xpack-js",
  "dependencies": {
    "xpm": "latest",
    "xmake": "latest",
    "xsvd": "latest"
  },
  "devDependencies": {
    "standard": "*"
  },
  "peerDependencies": {},
  "bundledDependencies": [],
  "optionalDependencies": {},
  "engines": {
    "node": ">7.7.0"
  }
}
